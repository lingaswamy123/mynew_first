#!/usr/bin/env nodejs
var http = require('http');
http.createServer(function (req, res) {
  res.writeHead(200, {'Content-Type': 'text/plain'});
  res.end('Hello World\n');
}).listen(8080, '{{ ansible_all_ipv6_addresses }}');
console.log('Server running at http://{{ ansible_all_ipv6_addresses }}:8080/');